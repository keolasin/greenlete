    "url": "https://github.com/keolasin/greenlete/issues"
  },
  "homepage": "https://github.com/keolasin/greenlete#readme",
  "devDependencies": {
    "husky": "^3.0.1",
    "jasmine": "^3.4.0",
    "nodemon": "^1.19.1",
    "prettier": "1.18.2",
